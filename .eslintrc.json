{
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": [
        ""
    ],
    "extends": [
        "standard-with-typescript",
        ""
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "no-useless-constructor": "off",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "off"
            }
        ]
    }
}